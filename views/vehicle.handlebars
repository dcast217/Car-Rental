<div class="container">
  {{#each vehicleData as |car|}}
  <p>
    {{brand}}<br />
    {{make}}
  {{/each}}
  </p>
</div>
<script id="vehicle-template" type="text/x-handlebars-template">
  <style>
    .container {
      background-color: lightgray;
      padding: 20px;
      border-radius: 5px;
    }

    .container p {
      font-size: 18px;
      margin-bottom: 10px;
    }

    .container p:last-child {
      margin-bottom: 0;
    }

    .brand {
      font-weight: bold;
    }
  </style>

  <div class="container">
    {{#each vehicleData as |car|}}
      <p>
        <span class="brand">{{car.brand}}</span><br />
        {{car.make}}
      </p>
    {{/each}}
  </div>
</script>